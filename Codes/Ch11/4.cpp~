#include"bits/stdc++.h"

using namespace std;


int main(){

    int N;
    while(cin>>N){
        int *arr=new int[N]();
        for(int i=0;i<N;i++)scanf("%d",&arr[i]);
        //input
        for(int i=0;i<N;i++){
            if(arr[i]%2==0)
                make_happen();
        } 
    }


return 0;}
